<% include ../partials/header %>

<div class="table table-hover">
  <h1 class="publi-title"> My Publications </h1>
  <a class="nav-item nav-link" href='/publications/new'>Add New</a>

  <div class="clearfix"><% pubdata.forEach(function(publi) { %>
  <div class="spanclass">
    <h5 class="publiname"><%= publi.content %></h5>
    <span class="paragraph">
    <p>Author: <%= publi.author %></p>
    <p><iframe src="<%= publi.genre_type %>" style="width:600px; height:500px;" frameborder="0">Click</iframe></p>
    </span>

        <a href="/publications/<%= publi.id %>" class="publibutton">View</a>

    </div>
  <% }) %></div>
</div>


<% include ../partials/footer %>




