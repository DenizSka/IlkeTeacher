<% include ../partials/header %>

<div class="table table-hover">
  <h1 class="publi-title"> My Publications </h1>


  <div class="clearfix publicationslist"><% pubdata.forEach(function(publi) { %>
  <div class="spanclass">
  <h5 class="publiname"><%= publi.content %></h5>
  <br></br>
    <span class="paragraph">
    <p>Publish Year: <%= publi.year %></p>
    <p>Author: <%= publi.author %></p>
    <p class="publiname-single"><p><iframe src="<%= publi.pdf %>" style="width:100px; height:100px;" frameborder="0"></iframe></p>

    </span>

        <a href="/publications/<%= publi.id %>" class="publibutton">View</a>

    </div>
  <% }) %></div>
</div>


<% include ../partials/footer %>




