<% include ../partials/header-signedin %>
<br></br>
<table class="table table-hover">
  <h1 class="headline"> Pending Users</h1>
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
      <th scope="col">Status</th>
    </tr>
  </thead>
  <tbody><% pendingusers.forEach(function(pendinguser) { %>
    <tr>
      <th scope="row"><%= pendinguser.id %></th>
      <td><%= pendinguser.fullname %></td>
      <td><%= pendinguser.email %></td>
      <td><a href='/login/1/pending/<%= pendinguser.id %>'> Accept | Decline </a></td>
    </tr>
  <% }) %></tbody>
</table>
</div>
<br>

<% include ../partials/footer %>
